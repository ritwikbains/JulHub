if not isfolder("JulHub") then
	makefolder("JulHub")
end

if not isfolder("JulHub/JulHubData") then
	makefolder("JulHub/JulHubData")
end

if game.PlaceId == 920587237 then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/JulHubz/JulHub/main/AdoptMe"))()
elseif game.PlaceId == 4924922222 then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/JulHubz/JulHub/main/Brookhaven"))()
end
